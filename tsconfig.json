{
	"compilerOptions": {
		"target": "ES2022",
		"module": "CommonJS",
		"outDir": "./build",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "node16"
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules"]
}
